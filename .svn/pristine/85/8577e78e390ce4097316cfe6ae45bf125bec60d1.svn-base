/*User Time*/

.user-time-table {
    position: relative;
    overflow: hidden;
    width: 100%;
    border-collapse: collapse;
    text-align: center;
    color: #3d3d3f;
    background: #fff;
}
.user-time-table .table-title {
    height: 35px;
    color: #fff;
    background: #63adff;
}
.table-title th {
    padding: 0;
    border-color: #50e3c2;
}
.user-time-table td {
    line-height: 55px;
    height: 55px;
    border: 1px solid #eee;
}
.user-time-table tr > td:nth-child(odd) {
    background: #f7f7f7;
}
.user-time-table tr:last-child td {
    border-bottom: none;
}
table.user-time-table:after {
    position: absolute;
    z-index: 15;
    top: auto;
    right: auto;
    bottom: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 1px;
    content: '';
    -webkit-transform: scaleY(.5);
    -ms-transform: scaleY(.5);
    transform: scaleY(.5);
    -webkit-transform-origin: 50% 100%;
    -ms-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
    background-color: #c8c7cc;
}
.w-16 {
    width: 16%;
}
.w-22 {
    width: 22%;
}
.w-40 {
    width: 40%;
}
.user-time-table input, .user-time-table textarea {
    font-size: 14px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    width: 100%;
    height: 100%;
    padding: 0 6px;
    text-align: center;
    border: none;
    background: transparent;
}
.user-time-table input.time-add {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.my-list {
    margin-top: 60px;
}

/*Picker*/

.picker-modal {
    background: #f7f7f7;
}
.toolbar {
    background: #f0f1f2;
}
.picker-modal .toolbar:before {
    background: #cdcdcd;
}
.picker-modal .toolbar:after {
    position: absolute;
    z-index: 15;
    top: auto;
    right: auto;
    bottom: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 1px;
    content: '';
    -webkit-transform-origin: 50% 0;
    -ms-transform-origin: 50% 0;
    transform-origin: 50% 0;
    background-color: #cdcdcd;
}

/*Modal*/

.modal .modal-title {
    font-size: 17px;
