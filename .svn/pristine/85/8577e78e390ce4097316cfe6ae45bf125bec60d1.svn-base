.hide {
    visibility: hidden;
}
input, textarea {
    -webkit-appearance: none;
}
textarea {
    resize: none;
}
ul, ol {
    margin: 0;
    padding: 0;
    list-style: none;
}
.clearfix:before, .clearfix:after {
    display: table;
    content: ' ';
}
.clearfix:after {
    clear: both;
}
.right {
    float: right;
}
.left {
    float: left;
}
.show {
    display: block!important;
}
.flipy {
    -webkit-transform: scaleY(-1);
    -ms-transform: scaleY(-1);
    transform: scaleY(-1);
    /*IE*/
    -webkit-filter: FlipV;
    filter: FlipV;
}

/*icon*/

i.icon.icon-oval {
    width: 29px;
    height: 29px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
