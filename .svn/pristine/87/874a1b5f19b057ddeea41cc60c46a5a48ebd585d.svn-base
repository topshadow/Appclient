      <script>
          $$('#logoff').on('click', function () {

              myApp.confirm('', '退出当前帐号，将不能<br/>实时查看客户留言', function () {
                  $.Frame.Ajax.Ajax({
                      url: $.Frame.Config.Constant.ServerUrl + "doctor.doctorlogoff.go",
                      postdata: {
                          doctorsno: localStorage.getItem("doctorSno"),
                          userType: "Doctor",
                          deviceType: G_DEVICE_TYPE,
                          registrationId: G_JPUSH_REGISTR_ID
                      },
                      success: function (rtn) {
                          if (rtn.issuccess) {
                              myApp.alert("登出成功",function(){
                                  window.location.href = "index.html";
                                  localStorage.clear();
                              });
                          } else {
                              myApp.alert("登出失败");
                          }
                      },
                      fail:function(){
                          myApp.alert("ERROR");
                      }
                  });
              });
          });
          $(function() {
