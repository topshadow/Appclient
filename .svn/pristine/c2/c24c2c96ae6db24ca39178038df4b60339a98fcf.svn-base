.messages-content .messages{ margin-bottom: 50px; }
.messages-auto-layout .message-avatar { opacity: 1; }
.messages-date span {
    line-height: 26px;
    display: block;
    width: 100px;
    height: 26px;
    margin: 0 auto;
    text-align: center;
    color: #fff;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    background-color: #cecece;
    padding-left: 10px;
    padding-right: 10px;
}

/*输入框*/
#chat-input {
    position: fixed;
    width: 100%;
    left: 0px;
    bottom: 0px;
    background-color: #e6e6e6;
    border-top: 1px  solid #ccc;
    height: 60px;
}
#chat-input input {
    margin: 10px 0;
    padding: 0 10px;
    width: 65%;
    width: -moz-calc(100% - 75px);
    height: 40px;
    border-radius: 10px;
    border: 1px solid #aeaeae;
    float: left;
    color: #333;
    font-size: 18px;
    background-color: #fff;
}

/*图片按钮*/
.send-images{
    width: 30px;
    height: 30px;
    padding: 0;
    border: 0;
    outline: none;
    float:left;
    margin:15px 5px 15px 5px;
}
.send-images img{
    width: 100%;
    height: 100%;
}

.btn{
    margin: 10px 0;
    padding: 6px 12px;
    height: 40px;
    background-color: #fff;
    border: 1px solid #999;
    border-radius: 8px;
    font-size: 18px;
    float: right;
    color: #333;
    margin-right:5px;
}


#send-cancel {
    height:24px;
    width:50px;
    font-size:14px;
    margin-top:8px;
    border-radius:3px;
    border:0;
    margin-left:10px;
}

#uploadImage {
    height:24px;
    width:50px;
    font-size:14px;
    margin-top:8px;
    border-radius:3px;
    border:0;
    background-color:#45c018;
    color:white;
    float:right;
    margin-right:10px;
}

