                        $(".user-time-table").find("tr").eq(3).find("td").eq(3).find("input").val(),
                        $(".user-time-table").find("tr").eq(4).find("td").eq(3).find("input").val(),
                        $(".user-time-table").find("tr").eq(5).find("td").eq(3).find("input").val(),
                        $(".user-time-table").find("tr").eq(6).find("td").eq(3).find("input").val(),
                        $(".user-time-table").find("tr").eq(7).find("td").eq(3).find("input").val()
                    );
                for (i = 0; i < 7; i++) {
                    if (am_orderquantity[i] == am_orderquantityold[i] && pm_orderquantity[i] == pm_orderquantityole[i] && place[i] == placeold[i]) {
                        if (i == 6) {
                            history.back();
                        }
                    }
                    else{
                        myApp.confirm('', '是否保存编辑？',
                   //点击确定
                   function () {
                       myApp.showPreloader('保存中...');
                       //星期一
                       var monday = new Array();
                       for (i = 0; i < am_orderquantity[0]; i++) {
                           var arr = "";
                           arr = 8 + 4 / am_orderquantity[0] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           if (arr < 10) {
                               monday[i] = "0" + parseInt(arr) + ":" + arr1[1];
                           }
                           else {
                               monday[i] = parseInt(arr) + ":" + arr1[1];
                           }
                       }
                       for (i = 0; i < pm_orderquantity[0]; i++) {
                           var arr = "";
                           arr = 14 + 3.5 / pm_orderquantity[0] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           monday[parseInt(am_orderquantity[0]) + i] = parseInt(arr) + ":" + arr1[1]
                       }
                       //星期二
                       var tuesday = new Array();
                       for (i = 0; i < am_orderquantity[1]; i++) {
                           var arr = "";
                           arr = 8 + 4 / am_orderquantity[1] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           if (arr < 10) {
                               tuesday[i] = "0" + parseInt(arr) + ":" + arr1[1];
                           }
                           else {
                               tuesday[i] = parseInt(arr) + ":" + arr1[1];
                           }
                       }
                       for (i = 0; i < pm_orderquantity[1]; i++) {
                           var arr = "";
                           arr = 14 + 3.5 / pm_orderquantity[1] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           tuesday[parseInt(am_orderquantity[1]) + i] = parseInt(arr) + ":" + arr1[1]
                       }
                       //星期三
                       var wednesday = new Array();
                       for (i = 0; i < am_orderquantity[2]; i++) {
                           var arr = "";
                           arr = 8 + 4 / am_orderquantity[2] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           if (arr < 10) {
                               wednesday[i] = "0" + parseInt(arr) + ":" + arr1[1];
                           }
                           else {
                               wednesday[i] = parseInt(arr) + ":" + arr1[1];
                           }
                       }
                       for (i = 0; i < pm_orderquantity[2]; i++) {
                           var arr = "";
                           arr = 14 + 3.5 / pm_orderquantity[2] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           wednesday[parseInt(am_orderquantity[2]) + i] = parseInt(arr) + ":" + arr1[1]
                       }
                       //星期四
                       var thursday = new Array();
                       for (i = 0; i < am_orderquantity[3]; i++) {
                           var arr = "";
                           arr = 8 + 4 / am_orderquantity[3] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
