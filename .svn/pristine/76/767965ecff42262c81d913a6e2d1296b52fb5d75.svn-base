                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           thursday[parseInt(am_orderquantity[3]) + i] = parseInt(arr) + ":" + arr1[1]
                       }
                       //星期五
                       var friday = new Array();
                       for (i = 0; i < am_orderquantity[4]; i++) {
                           var arr = "";
                           arr = 8 + 4 / am_orderquantity[4] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           if (arr < 10) {
                               friday[i] = "0" + parseInt(arr) + ":" + arr1[1];
                           }
                           else {
                               friday[i] = parseInt(arr) + ":" + arr1[1];
                           }
                       }
                       for (i = 0; i < pm_orderquantity[4]; i++) {
                           var arr = "";
                           arr = 14 + 3.5 / pm_orderquantity[4] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           friday[parseInt(am_orderquantity[4]) + i] = parseInt(arr) + ":" + arr1[1]
                       }
                       //星期六
                       var saturday = new Array();
                       for (i = 0; i < am_orderquantity[5]; i++) {
                           var arr = "";
                           arr = 8 + 4 / am_orderquantity[5] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           if (arr < 10) {
                               saturday[i] = "0" + parseInt(arr) + ":" + arr1[1];
                           }
                           else {
                               saturday[i] = parseInt(arr) + ":" + arr1[1];
                           }
                       }
                       for (i = 0; i < pm_orderquantity[5]; i++) {
                           var arr = "";
                           arr = 14 + 3.5 / pm_orderquantity[5] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           saturday[parseInt(am_orderquantity[5]) + i] = parseInt(arr) + ":" + arr1[1]
                       }
                       //星期天
                       var sunday = new Array();
                       for (i = 0; i < am_orderquantity[6]; i++) {
                           var arr = "";
                           arr = 8 + 4 / am_orderquantity[6] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           if (arr < 10) {
                               sunday[i] = "0" + parseInt(arr) + ":" + arr1[1];
                           }
                           else {
                               sunday[i] = parseInt(arr) + ":" + arr1[1];
                           }
                       }
                       for (i = 0; i < pm_orderquantity[6]; i++) {
                           var arr = "";
                           arr = 14 + 3.5 / pm_orderquantity[6] * i;
                           var arr1 = (((arr - parseInt(arr)) / 100 * 60).toFixed(2)).split(".");
                           sunday[parseInt(am_orderquantity[6]) + i] = parseInt(arr) + ":" + arr1[1]
                       }
                       $.Frame.Ajax.Ajax({
                           url: $.Frame.Config.Constant.ServerUrl + "doctor.getdoctororderquantity.go",
                           postdata: {
                               doctorsno: localStorage.getItem("doctorSno"),
                           },
                           success: function (rtn) {
                               if (rtn.issuccess) {
                                   for (i = 0 ; i < 7 ; i++) {
                                       if ($(".user-time-table").find("tr").eq(i + 1).find("td").eq(1).find("input").val() < (am[i] - rtn.data[i].am)) {
                                           myApp.alert("星期" + (i + 1) + "上午已被预约" + (am[i] - rtn.data[i].am) + "单");
                                           return false;
                                       }
                                       else if ($(".user-time-table").find("tr").eq(i + 1).find("td").eq(2).find("input").val() < (pm[i] - rtn.data[i].pm)) {
